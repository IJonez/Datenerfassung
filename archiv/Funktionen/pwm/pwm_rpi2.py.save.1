import RPi.GPIO as GPIO
import time
GPIO.setmode(GPIO.BOARD)

GPIO.setup(16, GPIO.OUT)

p = GPIO.PWM(16,50)

p.start(0)

try:
    try:
        dutycycle = input('Drehzahl? \n')
    dutycycle = float(dutycycle)
    while True:
        p.ChangeDutyCycle(dutycycle)
        
except KeyboardInterrupt:
           pass
p.stop()
GPIO.cleanup
           
                   
